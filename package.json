{
  "name": "react-redux-ssr-webapp",
  "version": "1.0.0",
  "description": "React Redux based Server Side Rendered Web Application",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "APP_MODE=development pm2-dev src/server",
    "dev:build": "APP_MODE=development webpack --config webpack/webpack.config.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod::server": "pm2 kill && pm2 start src/server --no-daemon",
    "prod:build": "APP_MODE=production webpack --config webpack/webpack.config.js",
    "logs": "pm2 logs",
    "stop": "pm2 stop src/server"
  },
  "author": "Abhishek Mishra",
  "license": "MIT",
  "dependencies": {
    "@babel/register": "^7.9.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "lodash": "^4.17.15",
    "marked": "^1.1.0",
    "moment": "^2.27.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-items-carousel": "^2.8.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.0",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
